[run]
source = .
omit = 
    */tests/*
    */test_*
    test_*
    *test*.py
    venv/*
    .venv/*
    */venv/*
    */.venv/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:

[html]
directory = htmlcov
